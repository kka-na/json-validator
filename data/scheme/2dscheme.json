{
    "title":"2D Semantic Segmentation Label File",
    "version":1,
    "type":"object",
    "properties":{
        "information":{
            "type":"object",
            "properties":{
                "filename":{"type":"string"},
                "resolution":{"type":"array", "items":{"type":"integer"}, "maxItems":2}
            },
            "required":["filename", "resolution"]
        },
        "Scene_meta":{
            "type":"object",
            "properties":{
                "zone":{"type":"string"},
                "edge_case":{"type":"array", "items":{"type":"string"}},
                "scene_number":{"type":"string"}
            },
            "required":["zone","edge_case","scene_number"]
        },
        "Environmental_meta":{
            "type":"object",
            "properties":{
                "weather":{"type":"string"},
                "date":{"type":"string"},
                "time":{"type":"string"},
                "car_type":{"type":"string"},
                "traffic_Type":{"type":"integer"},
                "speed_type":{"type":"integer"},
                "road_type":{"type":"string"}
            },
            "required":["weather", "date", "time", "car_Type", "traffic_Type","speed_type","road_type"]
        },
        "annotations":{
            "type":"array",
            "items":{
                "type":"object",
                "properties":{
                    "polygon":{"type":"array", "items":{"type":"number"}},
                    "class":{"type": "string"}
                },
                "required":["polygon", "class"]
            }
        }
    }
}